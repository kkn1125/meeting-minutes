{
  "name": "meeting-minutes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "cross-env NODE_ENV=production vite",
    "build": "rm -rf dist/ && tsc && vite build",
    "create": "mkdir \"dist/meeting-minutes/view\" \"dist/meeting-minutes/add\" \"dist/meeting-minutes/update\"",
    "postbuild": "pnpm run create && cp dist/index.html dist/meeting-minutes/update/ && cp dist/index.html dist/meeting-minutes/view/ && cp dist/index.html dist/meeting-minutes/add/",
    "deploy": "pnpm build && touch dist/.nojekyll && cd dist/ && git init && git add -A && git commit -m \"gh-pages\" && git push https://github.com/kkn1125/meeting-minutes.git master:gh-pages -f",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.4",
    "@mui/material": "^5.15.4",
    "@mui/x-date-pickers": "^6.19.0",
    "@nextui-org/react": "^2.2.9",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2",
    "uuid": "^9.0.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
